<%   max = specs.keys.map {|s| s.length}.max %>
<%   specs.each do |name,value| %>
<%=     wrap(inline_title(name,value,max)) %>
<%   end %>
<% end %>
<% unless overview.empty? %>
<%=  hrule %>

<%=  section_header( "Overview:" ) %>
<%=  wrap(overview) %>
<% end %>

